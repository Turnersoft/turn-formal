  {
    "name": "SigmaAlgebra",
    "docs": "",
    "kind": "enum",
    "members": [
      {
        "name": "Borel",
        "type": "Unit",
        "docs": "Borel σ-algebra: Generated by open sets\nB(X) = σ(τ) where τ is the topology\nThis is the smallest σ-algebra containing all open sets"
      },
      {
        "name": "Lebesgue",
        "type": "Unit",
        "docs": "Lebesgue σ-algebra: Completion of Borel σ-algebra\nL(X) = {A ∪ N : A ∈ B(X), N ⊆ Z, Z ∈ B(X), μ(Z) = 0}\nContains all sets differing from Borel sets by null sets"
      },
      {
        "name": "PowerSet",
        "type": "Unit",
        "docs": "Power set: Collection of all subsets\nP(X) = {A : A ⊆ X}\nLargest possible σ-algebra on X"
      },
      {
        "name": "Generated",
        "type": "String",
        "docs": "Generated σ-algebra: σ(C) for some collection C\nSmallest σ-algebra containing C\nUseful for custom measurable structures"
      }
    ]
  },
  {
    "name": "MeasureProperty",
    "docs": "",
    "kind": "enum",
    "members": [
      {
        "name": "Finite",
        "type": "FinitePropertyVariant",
        "docs": "Finiteness: total measure is finite\nμ(X) < ∞"
      },
      {
        "name": "SigmaFinite",
        "type": "SigmaFinitePropertyVariant",
        "docs": "σ-finiteness: countable union of finite measure sets\nX = ⋃ᵢXᵢ with μ(Xᵢ) < ∞"
      },
      {
        "name": "Borel",
        "type": "BorelPropertyVariant",
        "docs": "Borel property: defined on Borel σ-algebra\nDomain is σ-algebra generated by open sets"
      },
      {
        "name": "Hausdorff",
        "type": "HausdorffPropertyVariant",
        "docs": "Hausdorff property: separation by measure\nFor x≠y, ∃ measurable E: x∈E, y∉E"
      },
      {
        "name": "Radon",
        "type": "RadonPropertyVariant",
        "docs": "Radon property: inner regularity\nμ(E) = sup{μ(K): K⊆E compact} for all measurable E"
      }
    ]
  },
  {
    "name": "RadonPropertyVariant",
    "docs": "",
    "kind": "enum",
    "members": [
      {
        "name": "Radon",
        "type": "Unit",
        "docs": "Inner regular measure\nμ(E) = sup{μ(K): K⊆E compact}"
      },
      {
        "name": "NonRadon",
        "type": "Unit",
        "docs": "Not Radon"
      }
    ]
  },
  {
    "name": "HausdorffPropertyVariant",
    "docs": "",
    "kind": "enum",
    "members": [
      {
        "name": "Hausdorff",
        "type": "Unit",
        "docs": "Points separable by open sets\nx≠y ⟹ ∃U,V open: x∈U, y∈V, U∩V=∅"
      },
      {
        "name": "NonHausdorff",
        "type": "Unit",
        "docs": "Not Hausdorff"
      }
    ]
  },
