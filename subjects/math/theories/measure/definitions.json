  {
    "name": "MeasureSpaceProperty",
    "docs": "",
    "kind": "enum",
    "members": [
      {
        "name": "FiniteMeasure",
        "type": "FiniteMeasurePropertyVariant",
        "docs": "Finite measure property\nRelated to total measure being finite"
      },
      {
        "name": "SigmaFinite",
        "type": "SigmaFinitePropertyVariant",
        "docs": "σ-finite measure property\nRelated to countable decomposition"
      },
      {
        "name": "Regular",
        "type": "RegularityPropertyVariant",
        "docs": "Regularity property\nRelated to inner/outer approximations"
      }
    ]
  },
  {
    "name": "RegularityPropertyVariant",
    "docs": "",
    "kind": "enum",
    "members": [
      {
        "name": "Regular",
        "type": "Unit",
        "docs": "Measure is regular\nFor all A ∈ Σ:\nμ(A) = sup{μ(K) : K ⊆ A, K compact} (inner regularity)\nμ(A) = inf{μ(U) : A ⊆ U, U open} (outer regularity)\nExamples: Lebesgue measure, Radon measures"
      },
      {
        "name": "NonRegular",
        "type": "Unit",
        "docs": "Measure is not regular\nLacks inner/outer approximations\nExamples: Some pathological measures"
      }
    ]
  },
  {
    "name": "FiniteMeasurePropertyVariant",
    "docs": "",
    "kind": "enum",
    "members": [
      {
        "name": "Finite",
        "type": "Unit",
        "docs": "Space has finite measure\nμ(X) < ∞\nExamples: Probability spaces, bounded intervals with Lebesgue measure"
      },
      {
        "name": "Infinite",
        "type": "Unit",
        "docs": "Space has infinite measure\nμ(X) = ∞\nExamples: Lebesgue measure on R, counting measure on ℕ"
      }
    ]
  },
  {
    "name": "SigmaFinitePropertyVariant",
    "docs": "",
    "kind": "enum",
    "members": [
      {
        "name": "SigmaFinite",
        "type": "Unit",
        "docs": "Space is σ-finite\nX = ∪ₙ Xₙ with μ(Xₙ) < ∞\nExamples: Lebesgue measure on R, counting measure on countable sets"
      },
      {
        "name": "NonSigmaFinite",
        "type": "Unit",
        "docs": "Space is not σ-finite\nCannot be written as countable union of finite measure sets\nExamples: Counting measure on uncountable sets"
      }
    ]
  },
